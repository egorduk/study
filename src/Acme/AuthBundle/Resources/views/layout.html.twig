    {% block stylesheets %}
        <link href="{{ asset('bundles/css/bootstrap.css') }}" rel="stylesheet" />
        <link href="{{ asset('bundles/css/bootstrap-theme.css') }}" rel="stylesheet" />
        <link href="{{ asset('bundles/css/font-awesome-4.0.3/css/font-awesome.css') }}" rel="stylesheet" />
        <link href="{{ asset('bundles/acmeindex/css/style.css') }}" rel="stylesheet" />
    {% endblock %}
    {% block javascripts %}
